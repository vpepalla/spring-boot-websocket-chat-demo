{
    "family": "%APP_NAME%",
    "containerDefinitions": [
        {
            "image": "%ECR_URI%:%BUILD_NUMBER%",
            "name": "%APP_NAME%",
            "cpu": 256,
            "memory": 512,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 9090
                }
            ]
        }
    ]
}
